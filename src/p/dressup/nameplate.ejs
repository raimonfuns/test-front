					<script type="text/html" id="name-plate">
					<div class="block-body" id="name-plate-bady">
                        <div class="block-fixed">
                            <div class="block-hidden">
                                <ul class="list-box" data-role="content">
                                {{each list as val key}}
                                    {{if key%14 == 0}}
                                    <li>
                                        <div class="card-box clearfix">
                                    {{/if}}
                                            <div class="card" price="{{list[key].price}}" type="{{list[key].dressUpType}}" pid="{{list[key].pid}}" free="{{list[key].is_free}}">
                                                {{if list[key].isNew }}
                                                <i class="new"></i>
                                                {{/if}}
                                                <img src="{{list[key].img_url}}" alt="{{list[key].name}}">
                                                {{if list[key].price!=0 }}
                                                <span class="price">{{list[key].price}}张V卡</span>
                                                {{/if}}
                                                <div class="card-menu">
                                                	<div class="card-bar">
                                                        <a class="gift" href="javascript:;" price="{{list[key].price}}" type="{{list[key].dressUpType}}" pid="{{list[key].pid}}"  free="{{list[key].is_free}}">赠送</a>
                                                        <a class="exchange" href="javascript:;"  price="{{list[key].price}}" type="{{list[key].dressUpType}}" pid="{{list[key].pid}}"  free="{{list[key].is_free}}">兑换</a>
                                                    </div>
                                                </div>
                                            </div>
                                    {{if (key+1)%14 == 0||(key+1)==list.length}}      
                                        </div>
                                    </li>
                                    {{/if}}
                                {{/each}}
                                </ul>
                            </div>
                            
                        </div>
                        <ul class="list-btn" id="nameplate-btn">
                            {{each list as val key}}
                            	{{if key%14 == 0}}<li data-role="trigger">{{/if}}
                            	{{if (key+1)%14 == 0||(key+1)==list.length}}</li>{{/if}}
                            {{/each}}
                        </ul>
                    </div>
                    </script>
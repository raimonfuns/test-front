.heart{
	bottom:0;
	right:0;
	position:absolute;
	width: 20px;
	height: 40px;
	padding:0 12px;
	background:#fff;
	opacity:0.9;
	filter:unquote('alpha(opacity=90)');
	overflow:hidden;
}

.heart a {
  position: relative;
  color: #aaa;
  text-decoration: none;
  font-size: 1.8em;
  -webkit-transition: all 0.4s ease-out;
  transition: all 0.4s ease-out;
  cursor:unquote(default);
  width: 20px;
  height:40px;
}

.heart a.active, .heart a.active:hover {
  color: #da3737;
}

.heart .not-love-a:hover {
  color: #909090;
  text-shadow: 0 2px 4px rgba(0, 0, 0, 0.2);
}

.heart a:before {
  content: attr(data-text);
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0.8;
}

.heart a.active:before {
  -webkit-animation: heart-bump 0.8s 1;
  animation: heart-bump 0.8s 1;
}

@keyframes heart-bump {
  0% {
    opacity: 0.8;
  }
  30% {
    -webkit-transform: rotate(20deg);
    transform: rotate(20deg);
  }
  60% {
    -webkit-transform: rotate(-20deg);
    transform: rotate(-20deg);
  }
  100% {
    opacity: 0;
    top: -30px;
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
  }
}
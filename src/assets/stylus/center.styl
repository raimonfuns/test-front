    #tab-box-redname{
        display:none;
    }
 .face-select h4,
    .face-preview h4 {
        /*border: 0;*/
    }
    .skin-select {
        float: left;
        width: 455px;
    }
    .skin-select__list {
        margin:7px 0 0 -3px;
        
        li {
            float: left;
            display: inline;
            margin: 0 4px 0 0;
            padding-bottom: 14px;
        }
        img {
            width: 100px;
            height: 100px;
            cursor: pointer;
        }
        a {
            float: left;
            width: 100px;
            height: 100px;
            padding: 3px;
        }
        a:hover {
            background: url(http://file.do.yy.com/group3/M01/59/54/tz0MYFTbAiuAFL1fAAA7InB1EJ8309.png) no-repeat -400px 0;
        }
        a.select-skin{
            background: url(http://file.do.yy.com/group3/M01/59/54/tz0MYFTbAiuAFL1fAAA7InB1EJ8309.png) no-repeat -400px 0;
        }
    }
    .skin-preview {
        float: left;
        width: 275px;
    }
    .skin-preview__img {
        margin: 10px 0 0;
        img {
            width: 275px;
        }
        .skin-preview_pinfo{
        	position:relative;
        	zoom:1;	
        	background:url(http://file.do.yy.com/group3/M01/59/54/tz0MYFTbAiuAFL1fAAA7InB1EJ8309.png) no-repeat;
        	padding:45px 10px 0 95px;
        	height:205px;
        }
        .skin-preview_pinfo_show{
        	position:absolute; 
        	top:44px; 
        	left:10px;
        	img{
        		display:block;
	        	width:72px;
	        	height:72px;
	        	-o-border-radius:60px;
	        	-ms-border-radius:60px;
	        	-moz-border-radius:60px;
	        	-webkit-border-radius:60px;
	        	border-radius:60px;
	        }
        }
        .skin-preview_pinfo-ns{
        	display:block;
        }
        .skin-preview_pinfo-name, .skin-preview_pinfo-sign{
        	display:block;
        	font-size:18px;
        	line-height:30px;
        	height:30px;
        	width:100%;
        	white-space:nowrap;
        	overflow:hidden;
        	text-overflow:ellipsis;
        	color:#FFFFFF;
        }
        .skin-preview_pinfo-sign{
        	font-size:12px;
        	color:#FFFFFF;
        	line-height:1.5;
        }
    }
    .skin-preview__show {
        margin: 0 0 50px;
        a {
            display: inline-block;
            zoom:1;
            vertical-align: top;
            width: 100px;
            height: 40px;
            line-height: 40px;
            color: #fff;
            background: #ff9000;
            text-align: center;
            font-size: 14px;
            font-family: "Microsoft Yahei";
            &:hover {
                text-decoration: none;
                color: #fff;
                background: #ffa525;
            }

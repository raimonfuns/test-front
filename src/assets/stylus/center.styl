//个人中心，我的特权
.center-privilege-box {
    &.box--big{
        margin: 0;
    }
    .tab-box__hd__tab a {
        width: 145px;
        outline:none;
    }
    .skin-select h4,.skin-preview h4,.face-select h4,.face-preview h4,.popup-select h4,.popup-preview h4,.infoskin-select h4,.infoskin-preview h4,.logoprecious-select h4,.logoprecious-preview h4{
        font-size: 14px;
        font-family: "Microsoft Yahei";
        font-weight: 400;
        border-bottom: 1px dotted #dbdbdb;
        padding-bottom: 25px;
        margin-bottom: 15px;
    }    
    .tab-box__bd{
        display:none;
        .mod-preview{
            &.fixed{
                position:fixed;
                top:0;
            }
        }

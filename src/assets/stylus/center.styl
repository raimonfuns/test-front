.setcodemod-tips strong{
	display:inline-block;
	width:16px;
	height:16px;
	overflow:hidden;
	text-indent:-999px;
	background:url(http://file.do.yy.com/group3/M02/5B/7A/tz0GSFV5BGeAGHrBAAHI6tsqg8M295.png) no-repeat -700px -600px;
	margin:0 3px;
	vertical-align:middle;
}

.setcodemod-h_1, .setcodemod-h_2, .setcodemod-h_3{
	font-size:18px;
	line-height:33px;
	padding:0 0 0 48px;
	background:url(http://file.do.yy.com/group3/M02/5B/7A/tz0GSFV5BGeAGHrBAAHI6tsqg8M295.png) no-repeat;
	margin:0 0 30px 0;
	font-weight:lighter;
}
.setcodemod-h_1{
	background-position:-700px 0;
}
.setcodemod-h_2{
	background-position:-700px -97px;
}
.setcodemod-h_3{
	background-position:-700px -200px;
}

.setcodemod-show{
	width:100%; 
	overflow:hidden;
	margin:0 0 40px 0;
}
.setcodemod-show-list{
	overflow:auto;
	zoom:1;
	width:110%;
}
.setcodemod-show-list li{
	float:left;
	width:226px;
	margin:0 25px 0 0;
}
.setcodemod-show-list_1, .setcodemod-show-list_2, .setcodemod-show-list_3{
	display:block;
	padding:235px 0 0 0;
	line-height:30px;
	background:url(http://file.do.yy.com/group3/M02/5B/7A/tz0GSFV5BGeAGHrBAAHI6tsqg8M295.png) no-repeat;
	text-align:center;
	font-weight:lighter;
}
.setcodemod-show-list_2{
	background-position: -300px 0;

}
.setcodemod-show-list_3{
	background-position: 0 -300px;
}

.setcodemod-manage{margin:0 0 40px 0;}
.setcodemod-manage-list{overflow:auto; zoom:1; padding:1px 0 0 1px;}
.setcodemod-manage-list li{
	float:left; 
	width:146px; 
	height:88px; 
	border:1px solid #f3f3f3; 
	color:#b1b1b1; 
	padding:0 0 0 95px; 
	position:relative; 
	zoom:1; 
	margin:-1px 0 0 -1px;
	background:url(http://file.do.yy.com/group3/M02/5B/7A/tz0GSFV5BGeAGHrBAAHI6tsqg8M295.png) no-repeat -684px -462px;
	cursor:pointer;
	overflow:hidden;
}
.setcodemod-manage-list li.selected{
	background-position:-684px -362px;
}
.setcodemod-manage-list-icon{position:absolute; padding:16px; border:1px solid #f3f3f3; top:20px; left:40px; width:16px; height:16px;}
.setcodemod-manage-list-icon img{display:block; width:16px; height:16px;}
.setcodemod-manage-list-name{color:#333333; display:block; padding:25px 0 0;}

.setcodemod-manage-btn{margin:-1px 0 0 0;text-align:right; height:62px; padding:18px 168px 0 0; line-height:40px; background:#fffcf3; border:1px solid #f3f3f3; font-size:12px; position:relative; zoom:1;}
.setcodemod-manage-btn span{}
.setcodemod-manage-btn strong{color:#d6090d; font-weight:lighter;}
.setcodemod-manage-btn-change{position:absolute; top:18px; right:40px; width:100px; text-align:center; line-height:40px; background:#ff9000; text-decoration:none !important; font-size:14px; color:#FFFFFF !important;}
.setcodemod-manage-btn-more{color:#d6090d !important;}

.setcodemod-rules{font-size:14px; color:#333333;}
.setcodemod-rules-list{border:1px solid #f3f3f3; padding:18px 0;}
.setcodemod-rules-list li{background:url(http://file.do.yy.com/group3/M02/5B/7A/tz0GSFV5BGeAGHrBAAHI6tsqg8M295.png) no-repeat -677px -290px; padding:0 0 0 35px; line-height:25px;}
.setcodemod-rules-list strong{font-weight:lighter; color:#d6090d;}
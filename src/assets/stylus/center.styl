//会员炫铃
.ring-box {
    margin: 0 0 20px;
}
.ring-box__title {
    font-size: 18px;
    font-family: "Microsoft Yahei";
    line-height: 38px;
    font-weight: 400;
    padding: 25px 0 28px;
    i{display: inline-block;zoom: 1;height: 38px;margin: 0 12px 0 0;width: 36px;vertical-align: -12px;background: url(../img/ring_icon.png) no-repeat;}
}
.ring-box__title--s2 {
    i {
        background-position: 0 -38px;
    }
}
.ring-box__title--s3 {
    i {
        background-position: 0 -76px;
    }
}
.ring-box__item {
    float: left;
    white-space: nowrap;
    margin:15px 20px 0 15px;
    width:150px;
    input {
        margin: 3px 5px 0 0;
        vertical-align: top;
    }
    a {
        display: inline-block;
        zoom:1;
        width:12px;
        height: 8px;
        background: url(../img/music-icon.png) no-repeat;
        vertical-align: top;
        margin: 5px 0 0 5px;
    }
    label {
        display: inline-block;
        zoom:1;
        vertical-align: top;
    }
}
.ring-box__ft {
    background: #fffcf3;
    border: 1px solid #f3f3f3;
    border-top: 0 none;
    padding: 18px 40px 20px;
    text-align: right;
}
.ring-box__select,
.ring-box__submit,
.ring-box__reset {
    display: inline-block;
    zoom:1;
    height: 40px;
    line-height: 40px;
    vertical-align: top;
    color: #fff;
}
.ring-box__submit {
    background: #ff9000;
    padding: 0 33px;
    margin: 0 20px 0 30px;
    font-size: 14px;
    font-family: "Microsoft Yahei";
    &:hover {
        text-decoration: none;
        background: #ffa525;
        color: #fff;
    }
}
.ring-box__reset {
    background: #d60a0d;
    padding: 0 17px;
    font-size: 14px;
    font-family: "Microsoft Yahei";
    &:hover {
        text-decoration: none;
        background: #f50a0e;
        color: #fff;
    }
}
.ring-box__select {
    color: #333;
    em {
        color: #d6090d;
    }
}

.ring-box__table {
    width: 100%;
    td,th {
        border: 1px solid #f3f3f3;
    }
    th {
        background: #fbfbfb;
        color: #d40c0f;
        font-weight: 400;
        vertical-align: middle;
        width: 120px;
        text-align: center;
    }
    td {
        padding-bottom: 15px;
    }
    .disable,
    .disable th {
        color: #b1b1b1;
    }
}


.ring-box__setting {
    overflow: hidden;
    zoom: 1;
    padding: 25px 0 10px;
    border: 1px solid #f3f3f3;
    .firend-box {
        float: left;
        width: 265px;
    }
    .firend-box__hd,
    .firend-box__bd {
        padding-left: 20px;
    }
    .remind-setting {
        float: left;
        display: inline;
        margin: 0 0 0 25px;
        width: 418px;
    }
    .remind-setting__hd {
        color: #b1b1b1;
        height: 23px;
        line-height: 23px;
        margin: 0 0 10px;
        a {
            float: right;
            color: #b1b1b1;
            &:hover {
                color: #f50a0e;
            }
        }
    }
    .remind-setting__bd {
        background: #f9f3dd;
        line-height: 22px;
        overflow-y: auto;
        height: 240px;
        table {
            width: 100%;
        }
        tbody {
            padding: 10px 0;
        }
        th {
            height: 32px;
            line-height: 32px;
            border-right: 1px solid #fff;
            padding: 0 0 0 20px;
            text-align: left;
        }
        td {
            background: #fefbf2;
            padding: 0 0 0 20px;
            line-height: 26px;
        }
        .remind-setting__name,
        .remind-setting__ring,
        .remind-setting__cancel {
            display: block;
            height: 26px;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
            width: 130px;
        }
        .remind-setting__ring {
            width: 150px;
             a {
                display: inline-block;
                zoom:1;
                width:12px;
                height: 8px;
                background: url(../img/music-icon.png) no-repeat;
                vertical-align: 0;
                margin: 0 5px 0 0;
            }
        }
        .remind-setting__cancel {
            color: #b1b1b1;
            width: auto;
            padding: 0 20px 0 0;
            &:hover {
                color: #d2090f;
            }
        }
    }
    .remind-setting__ft {
        text-align: right;
        padding-top: 10px;
        strong {
            font-weight: 700;
            color:#d50709;
        }
    }
}

//在线特权
.online-privilege__hd {
    background: #fbfbfb;
    border: 1px solid #f3f3f3;
    height: 73px;
    li {
        float: left;
        line-height: 74px;
        margin: 0 0 -1px;
        padding: 0 23px;
        color: #b1b1b1;
        cursor:pointer;
    }
    .current {
        background: #fff;
        color: #333;
    }
}
.online-privilege__bd {
    overflow: hidden;
    zoom:1;
    border: 1px solid #f3f3f3;
    border-top: 0;
    .firend-box {
        float: left;
        width: 285px;

    }
    .firend-box__hd {
        border-bottom: 1px solid #f3f3f3;
        height: 62px;
        padding: 0 23px;
        margin:0;
        select {
            margin-top: 20px;
        }
    }
    .firend-box__bd {
        background: #fff;
        padding: 10px 0 10px 23px;
        table {
            width: auto;
        }
    }
    .select-persion {
        float: left;
        width: 443px;
    }
    .select-persion__hd {
        border-bottom: 1px solid #f3f3f3;
        height: 62px;
        table {
            line-height: 62px;
            width: 100%;
        }
    }
    .select-persion__bd {
        border-left: 1px solid #f3f3f3;
        padding: 10px 0 10px;
        overflow-y: auto;
        overflow-x: hidden;
        height: 240px;
        table {
            width: 100%;
            text-align: center;
        }
        .setting__name,
        .setting__func,
        .setting__cancel {
            display: block;
            height: 22px;
            line-height: 22px;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
            width: 160px;
            padding: 0 0 0 30px;
        }
        .setting__func {
            width: 90px;
            adding: 0 0 0 10px;
        }
        .setting__cancel {
            width: 90px;
            color: #b1b1b1;
            adding: 0 0 0 10px;
            &:hover {
                color: #d2090f;
                text-decoration: none;
            }
        }

    }
    .select-persion__ft {
        text-align: right;
        padding: 10px 20px 10px 0;
        strong {
            font-weight: 700;
            color: #d50709;
        }
    }
}

